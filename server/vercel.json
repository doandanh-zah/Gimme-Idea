{
  "version": 2,
  "name": "gimme-idea-backend",
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/server.js"
    }
  ],
  "buildCommand": "npm run build",
  "installCommand": "npm install && npx prisma generate",
  "outputDirectory": "dist",
  "regions": ["sin1"]
}

